{
  "name": "@dyson-finance/dyson-interface-sdk",
  "description": "Dyson finance interface sdk",
  "keywords": [
    "dyson finance",
    "web3",
    "sdk"
  ],
  "license": "MIT",
  "version": "0.2.0",
  "type": "module",
  "homepage": "https://dyson.finance/",
  "contributors": [
    "Paul Li <paul.li@pelith.com>",
    "Tim Wang <timwang@pelith.com>",
    "Raman Chen <raman@pelith.com>"
  ],
  "files": [
    "/constants",
    "/actions",
    "/entities",
    "/reads",
    "/calculations",
    "/utils",
    "/dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    },
    "./actions": {
      "types": "./dist/actions/index.d.ts",
      "default": "./dist/actions/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "default": "./dist/entities/index.js"
    },
    "./reads": {
      "types": "./dist/reads/index.d.ts",
      "default": "./dist/reads/index.js"
    },
    "./calculations": {
      "types": "./dist/calculations/index.d.ts",
      "default": "./dist/calculations/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./constants/abis": {
      "types": "./dist/constants/abis/index.d.ts",
      "default": "./dist/constants/abis/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm version patch && pnpm run build",
    "clean": "rimraf ./dist",
    "test": "vitest run",
    "test:w": "vitest",
    "test:ui": "vitest --ui",
    "check": "tsc",
    "lint:fix": "eslint ./src --ext .jsx,.js,.ts,.tsx --quiet --fix --ignore-path ./.gitignore",
    "lint:format": "prettier  --loglevel warn --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" ",
    "lint": "yarn lint:format && yarn lint:fix ",
    "build:doc": "typedoc"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "@viem/anvil": "^0.0.6",
    "@vitest/ui": "^0.34.6",
    "dedent": "^1.5.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.12",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript": "^5.2.2",
    "vite-plugin-dts": "^3.5.3",
    "vitest": "^0.34.4"
  },
  "dependencies": {
    "@types/lodash-es": "^4.17.9",
    "abitype": "^0.9.8",
    "lodash-es": "^4.17.21"
  },
  "peerDependencies": {
    "viem": "^1.12.2"
  }
}
